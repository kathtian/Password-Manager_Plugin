{
  "manifest_version":3,
  "version":"1.0",
  "name":"Encryption Extension",
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "content_scripts":[
    {
      "matches":["<all_urls>"],
      "js":["encryption.js", "main.js"],
      "type": "module"
    }
  ]
}