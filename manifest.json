{
  "manifest_version":3,
  "version":"1.0",
  "name":"Encryption Extension",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts":[
    {
      "matches":["<all_urls>"],
      "js":["main.js"],
      "type": "module"
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}